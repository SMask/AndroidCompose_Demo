<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AndroidCompose_Demo">
        <activity
            android:name=".activity.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.AndroidCompose_Demo">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.CommonViewActivity"
            android:exported="false"
            android:label="@string/title_activity_common_view"
            android:theme="@style/Theme.AndroidCompose_Demo" />
        <activity
            android:name=".activity.DraggableViewActivity"
            android:exported="false"
            android:label="@string/title_activity_draggable_view"
            android:theme="@style/Theme.AndroidCompose_Demo" />
        <activity
            android:name=".activity.StateActivity"
            android:exported="false"
            android:label="@string/title_activity_state"
            android:theme="@style/Theme.AndroidCompose_Demo" />
    </application>

</manifest>